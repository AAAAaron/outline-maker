{
	"defaultSeverity": "error",
	"extends": [
		"tslint-config-airbnb",
		"tslint-react"
	],
	"jsRules": {},
	"rules": {
		"max-line-length": [
			true,
			120
		],
		"trailing-comma": false,
		"indent": [
			true,
			"tabs",
			2
		],
		"ter-indent": [
			false
		],
		"variable-name": false,
		"jsx-alignment": true,
		"jsx-equals-spacing": [
			true,
			"never"
		],
		"jsx-no-string-ref": true,
		"jsx-self-close": true,
		"jsx-no-multiline-js": false,
		"jsx-boolean-value": false,
		"import-name": false,
		"jsx-no-lambda": false,
		"function-name": false,
		"no-increment-decrement": false
	},
	"rulesDirectory": []
}